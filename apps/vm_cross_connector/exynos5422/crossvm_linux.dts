		i2c@12cb0000 {
			compatible = "samsung,exynos5250-hsi2c";
			reg = <0x12cb0000 0x1000>;
			interrupts = <0x0 0x3d 0x0>;
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			status = "okay";
			clocks = <0x2 0x10a>;
			clock-names = "hsi2c";
			pinctrl-names = "default";
			pinctrl-0 = <0x1f>;
			samsung,hs-mode;
			clock-frequency = <0x61a80>;

			sx865x@49 {
				status = "disabled";
				compatible = "semtech,sx8650";
				reg = <0x49>;
				#clock-cells = <0x0>;
				gpio-pendown = <0x20 0x1 0x0>;
				gpio-reset = <0x21 0x5 0x1>;
				invert-x = <0x0>;
				invert-y = <0x1>;
