/dts-v1/;

/ {
	interrupt-parent = <0x1>;
	#address-cells = <0x1>;
	#size-cells = <0x1>;
	compatible = "hardkernel,odroid-xu4", "samsung,exynos5800", "samsung,exynos5";
	model = "Hardkernel Odroid XU4";

	soc {
		compatible = "simple-bus";
		#address-cells = <0x1>;
		#size-cells = <0x1>;
		ranges;

		syscon-poweroff {
			compatible = "syscon-poweroff";
			regmap = <0x3>;
			offset = <0x330c>;
			mask = <0x5200>;
		};

		syscon-reboot {
			compatible = "syscon-reboot";
			regmap = <0x3>;
			offset = <0x400>;
			mask = <0x1>;
		};

		chipid@10000000 {
			compatible = "samsung,exynos4210-chipid";
			reg = <0x10000000 0x100>;
		};

		memory-controller@12250000 {
			compatible = "samsung,exynos4210-srom";
			reg = <0x12250000 0x14>;
		};

		interrupt-controller@10440000 {
			compatible = "samsung,exynos4210-combiner";
			#interrupt-cells = <0x2>;
			interrupt-controller;
			samsung,combiner-nr = <0x20>;
			reg = <0x10440000 0x1000>;
			interrupts = <0x0 0x0 0x0 0x0 0x1 0x0 0x0 0x2 0x0 0x0 0x3 0x0 0x0 0x4 0x0 0x0 0x5 0x0 0x0 0x6 0x0 0x0 0x7 0x0 0x0 0x8 0x0 0x0 0x9 0x0 0x0 0xa 0x0 0x0 0xb 0x0 0x0 0xc 0x0 0x0 0xd 0x0 0x0 0xe 0x0 0x0 0xf 0x0 0x0 0x10 0x0 0x0 0x11 0x0 0x0 0x12 0x0 0x0 0x13 0x0 0x0 0x14 0x0 0x0 0x15 0x0 0x0 0x16 0x0 0x0 0x17 0x0 0x0 0x18 0x0 0x0 0x19 0x0 0x0 0x1a 0x0 0x0 0x1b 0x0 0x0 0x1c 0x0 0x0 0x1d 0x0 0x0 0x1e 0x0 0x0 0x1f 0x0>;
			linux,phandle = <0xf>;
			phandle = <0xf>;
		};

		interrupt-controller@10481000 {
			compatible = "arm,gic-400", "arm,cortex-a15-gic", "arm,cortex-a9-gic";
			#interrupt-cells = <0x3>;
			interrupt-controller;
			reg = <0x10481000 0x1000 0x10482000 0x2000 0x10484000 0x2000 0x10486000 0x2000>;
			interrupts = <0x1 0x9 0xf00>;
			linux,phandle = <0x1>;
			phandle = <0x1>;
		};

		syscon@10050000 {
			compatible = "samsung,exynos5-sysreg", "syscon";
			reg = <0x10050000 0x5000>;
			linux,phandle = <0x6>;
			phandle = <0x6>;
		};

		serial@12C00000 {
			compatible = "samsung,exynos4210-uart";
			reg = <0x12c00000 0x100>;
			interrupts = <0x0 0x33 0x0>;
			clocks = <0x2 0x101 0x2 0x80>;
			clock-names = "uart", "clk_uart_baud0";
			dmas = <0x4 0xd 0x4 0xe>;
			dma-names = "rx", "tx";
		};

		serial@12C10000 {
			compatible = "samsung,exynos4210-uart";
			reg = <0x12c10000 0x100>;
			interrupts = <0x0 0x34 0x0>;
			clocks = <0x2 0x102 0x2 0x81>;
			clock-names = "uart", "clk_uart_baud0";
			dmas = <0x5 0xf 0x5 0x10>;
			dma-names = "rx", "tx";
		};

		serial@12C20000 {
			compatible = "samsung,exynos4210-uart";
			reg = <0x12c20000 0x100>;
			interrupts = <0x0 0x35 0x0>;
			clocks = <0x2 0x103 0x2 0x82>;
			clock-names = "uart", "clk_uart_baud0";
			dmas = <0x4 0xf 0x4 0x10>;
			dma-names = "rx", "tx";
		};

		serial@12C30000 {
			compatible = "samsung,exynos4210-uart";
			reg = <0x12c30000 0x100>;
			interrupts = <0x0 0x36 0x0>;
			clocks = <0x2 0x104 0x2 0x83>;
			clock-names = "uart", "clk_uart_baud0";
			dmas = <0x5 0x11 0x5 0x12>;
			dma-names = "rx", "tx";
		};

		i2c@12C60000 {
			compatible = "samsung,s3c2440-i2c";
			reg = <0x12c60000 0x100>;
			interrupts = <0x0 0x38 0x0>;
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			samsung,sysreg-phandle = <0x6>;
			status = "disabled";
			clocks = <0x2 0x105>;
			clock-names = "i2c";
			pinctrl-names = "default";
			pinctrl-0 = <0x7>;
		};

		i2c@12C70000 {
			compatible = "samsung,s3c2440-i2c";
			reg = <0x12c70000 0x100>;
			interrupts = <0x0 0x39 0x0>;
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			samsung,sysreg-phandle = <0x6>;
			status = "okay";
			clocks = <0x2 0x106>;
			clock-names = "i2c";
			pinctrl-names = "default";
			pinctrl-0 = <0x8>;
			samsung,i2c-max-bus-freq = <0x61a80>;
		};

		i2c-hdmi {
			compatible = "i2c-gpio";
			gpios = <0x9 0x6 0x0 0x9 0x7 0x0>;
			#i2c-gpio,sda-open-drain;
			#i2c-gpio,scl-open-drain;
			#i2c-gpio,scl-output-only;
			i2c-gpio,delay-us = <0x5>;
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			status = "okay";
			pinctrl-names = "default";
			pinctrl-0 = <0xa>;
			linux,phandle = <0x4f>;
			phandle = <0x4f>;
		};

		i2c@12C90000 {
			compatible = "samsung,s3c2440-i2c";
			reg = <0x12c90000 0x100>;
			interrupts = <0x0 0x3b 0x0>;
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			samsung,sysreg-phandle = <0x6>;
			status = "disabled";
			clocks = <0x2 0x108>;
			clock-names = "i2c";
			pinctrl-names = "default";
			pinctrl-0 = <0xb>;
		};

		pwm@12DD0000 {
			compatible = "samsung,exynos4210-pwm";
			reg = <0x12dd0000 0x100>;
			samsung,pwm-outputs = <0x0 0x2>;
			#pwm-cells = <0x3>;
			clocks = <0x2 0x117>;
			clock-names = "timers";
			pinctrl-0 = <0xc 0xd>;
			pinctrl-names = "default";
			status = "okay";
			linux,phandle = <0x98>;
			phandle = <0x98>;
		};

		rtc@101E0000 {
			compatible = "samsung,s3c6410-rtc";
			reg = <0x101e0000 0x100>;
			interrupts = <0x0 0x2b 0x0 0x0 0x2c 0x0>;
			status = "okay";
			clocks = <0x2 0x13d 0xe 0x0>;
			clock-names = "rtc", "rtc_src";
			interrupt-parent = <0x3>;
		};

		fimd@14400000 {
			compatible = "samsung,exynos5420-fimd";
			interrupt-parent = <0xf>;
			reg = <0x14400000 0x40000>;
			interrupt-names = "fifo", "vsync", "lcd_sys";
			interrupts = <0x12 0x4 0x12 0x5 0x12 0x6>;
			samsung,sysreg = <0x6>;
			status = "disabled";
			clocks = <0x2 0x93 0x2 0x1a5>;
			clock-names = "sclk_fimd", "fimd";
			power-domains = <0x10>;
			iommus = <0x11 0x12>;
			iommu-names = "m0", "m1";
		};

		dp-controller@145B0000 {
			compatible = "samsung,exynos5-dp";
			reg = <0x145b0000 0x1000>;
			interrupts = <0xa 0x3>;
			interrupt-parent = <0xf>;
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			status = "disabled";
			clocks = <0x2 0x19c>;
			clock-names = "dp";
			phys = <0x13>;
			phy-names = "dp";
			power-domains = <0x10>;
		};

		sysram@02020000 {
			compatible = "mmio-sram";
			reg = <0x2020000 0x54000>;
			#address-cells = <0x1>;
			#size-cells = <0x1>;
			ranges = <0x0 0x2020000 0x54000>;

			smp-sysram@0 {
				compatible = "samsung,exynos4210-sysram";
				reg = <0x0 0x1000>;
			};

			smp-sysram@53000 {
				compatible = "samsung,exynos4210-sysram-ns";
				reg = <0x53000 0x1000>;
			};
		};

		watchdog@101d0000 {
			compatible = "samsung,exynos5420-wdt";
			reg = <0x101d0000 0x100>;
			interrupts = <0x0 0x2a 0x0>;
			clocks = <0x2 0x13c>;
			clock-names = "watchdog";
			samsung,syscon-phandle = <0x3>;
		};

		arm-a7-pmu {
			compatible = "arm,cortex-a7-pmu";
			interrupt-parent = <0x1>;
			interrupts = <0x0 0xa0 0x0 0x0 0xa1 0x0 0x0 0xa2 0x0 0x0 0xa3 0x0>;
			status = "okay";
			interrupt-affinity = <0x14 0x15 0x16 0x17>;
		};

		arm-a15-pmu {
			compatible = "arm,cortex-a15-pmu";
			interrupt-parent = <0xf>;
			interrupts = <0x1 0x2 0x7 0x0 0x10 0x6 0x13 0x2>;
			status = "okay";
			interrupt-affinity = <0x18 0x19 0x1a 0x1b>;
		};

		sss@10830000 {
			compatible = "samsung,exynos4210-secss";
			reg = <0x10830000 0x300>;
			interrupts = <0x0 0x70 0x0>;
			clocks = <0x2 0x1d7>;
			clock-names = "secss";
		};

		i2c@12ca0000 {
			compatible = "samsung,exynos5250-hsi2c";
			reg = <0x12ca0000 0x1000>;
			interrupts = <0x0 0x3c 0x0>;
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			status = "okay";
			clocks = <0x2 0x109>;
			clock-names = "hsi2c";
			pinctrl-names = "default";
			pinctrl-0 = <0x1c>;

			s2mps11_pmic@66 {
				compatible = "samsung,s2mps11-pmic";
