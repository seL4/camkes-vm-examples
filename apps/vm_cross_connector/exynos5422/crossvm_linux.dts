		compatible = "samsung,secure-firmware";
		reg = <0x2073000 0x1000>;
	};

	fixed-rate-clocks {

		oscclk {
			compatible = "samsung,exynos5420-oscclk";
			clock-frequency = <0x16e3600>;
		};

		xxti {
			compatible = "samsung,clock-xxti";
			clock-frequency = <0x16e3600>;
		};
	};

	gpio_keys {
		compatible = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <0x95>;

		power_key {
			gpios = <0x1d 0x3 0x1>;
			linux,code = <0x74>;
			label = "power key";
			debounce-interval = <0x0>;
			wakeup-source;
		};
	};

	gpiomem {
		compatible = "samsung,exynos-gpiomem";
		reg = <0x13400000 0x1000 0x14010000 0x1000>;
		status = "okay";
	};

	pwrseq {
		pinctrl-0 = <0x96>;
		pinctrl-names = "default";
		compatible = "mmc-pwrseq-emmc";
		reset-gpios = <0x97 0x0 0x1>;
		linux,phandle = <0x2d>;
		phandle = <0x2d>;
	};

	pwm-fan {
		compatible = "pwm-fan";
		pwms = <0x98 0x0 0x51ec 0x0>;
		cooling-min-state = <0x0>;
		cooling-max-state = <0x3>;
		#cooling-cells = <0x2>;
		cooling-levels = <0x0 0x78 0xb4 0xf0>;
		linux,phandle = <0x74>;
		phandle = <0x74>;
	};

	1-wire {
		compatible = "w1-gpio";
		gpios = <0x21 0x0 0x0>;
		pinctrl-names = "default";
		pinctrl-0 = <0x99>;
		status = "okay";
	};

	extcon_usb3_0 {
		compatible = "linux,extcon-usb-gpio";
		vbus-gpio = <0x20 0x5 0x0>;
		pinctrl-names = "default";
		pinctrl-0 = <0x9a>;
		linux,phandle = <0x27>;
		phandle = <0x27>;
	};

	extcon_usb3_1 {
		compatible = "linux,extcon-usb-gpio";
		id-gpio = <0x9b 0x1 0x0>;
		vbus-gpio = <0x20 0x4 0x0>;
		pinctrl-names = "default";
		pinctrl-0 = <0x9c 0x9d>;
		linux,phandle = <0x29>;
		phandle = <0x29>;
	};

	pwmleds {
		compatible = "pwm-leds";

		blueled {
			label = "blue:heartbeat";
			pwms = <0x98 0x2 0x1e8480 0x0>;
			pwm-names = "pwm2";
			max_brightness = <0xff>;
			linux,default-trigger = "heartbeat";
		};
	};

	spdif-transmitter {
		compatible = "linux,spdif-dit";
		#sound-dai-cells = <0x0>;
		status = "okay";
		linux,phandle = <0x9f>;
		phandle = <0x9f>;
	};

	sound {
		compatible = "samsung,odroid-xu3-audio";
		model = "Odroid-XU4";
		clocks = <0x2 0x5 0x2 0x291 0x2 0x292 0x2 0x293 0x3e 0x0 0x3e 0x1 0x3e 0x2 0x3e 0x3 0x2 0x9f>;
		clock-names = "fout_epll", "mout_sclk_epll", "mout_mau_epll", "mout_mau_epll_user", "mout_audss", "mout_i2s", "dout_srp", "dout_aud_bus", "mau_epll_clk";

		cpu {
			sound-dai = <0x9e 0x0>;
		};

		codec {
			sound-dai = <0x9f 0x53>;
		};
	};

	timer {
		compatible = "arm,cortex-a15-timer", "arm,armv7-timer";
		interrupts = <0x1 0xd 0xf08 0x1 0xe 0xf08 0x1 0xb 0xf08 0x1 0xa 0xf08>;
		clock-frequency = <0x16e3600>;
	};

    pci {
        compatible = "pci-host-cam-generic";
        device_type = "pci";
        #address-cells = <0x3>;
        #size-cells =  <0x2>;
        bus-range = <0x0 0x1>;
        dma-coherent;
        #interrupt-cells = <0x1>;

        reg = <0x3e000000 0x1000000>;
        ranges = <0x01000000 0x0 0x00000000 0x3d000000 0x0 0x00010000>,
                 <0x02000000 0x0 0x3f000000 0x3f000000 0x0 0x1000000>;

        interrupt-map = <0x800  0 0 1 0x1 0  92  4
                         0x1000 0 0 1 0x1 0  92  4
                         0x1800 0 0 1 0x1 0  92  4
                         0x2000 0 0 1 0x1 0  92  4
                         0x2800 0 0 1 0x1 0  92  4>;

        interrupt-map-mask = <0x0000f800 0x00000000 0x00000000 0x00000007>;
    };

};
